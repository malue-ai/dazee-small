{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "默认权限配置",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "notification:default",
    "updater:default",
    "dialog:default",
    "shell:allow-open",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "binaries/xiaodazi-backend",
          "sidecar": true,
          "args": true
        }
      ]
    },
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "binaries/xiaodazi-backend",
          "sidecar": true,
          "args": true
        }
      ]
    }
  ]
}
